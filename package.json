{
  "name": "nulp-timetable",
  "version": "0.0.1",
  "description": "NULP timetable",
  "main": "''",
  "engines": {
    "node": "10.12.0"
  },
  "scripts": {
    "clean": "shx rm -rf build",
    "webpack": "webpack --config config/webpack.config.production.js",
    "sass": "node-sass src/styles/index.scss build/index.css --output-style compressed",
    "build": "npm run clean && npm run sass && npm run webpack",
    "lint": "eslint src config",
    "dev": "del-cli --force ./build && webpack-dashboard -- webpack-dev-server --config config/webpack.config.dev.js",
    "start": "serve -s build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/prettydude/nulp-timetable"
  },
  "author": "prettydude",
  "license": "MIT",
  "homepage": "https://prettydude.github.io/nulp-timetable",
  "dependencies": {
    "history": "^4.10.1",
    "idb": "^5.0.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-search-panel": "^1.0.8"
  },
  "jest": {
    "verbose": true,
    "bail": true,
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "moduleNameMapper": {
      "\\.(jpe?g|png|gif|eot|otf|webp|svg|ttf|woff2?|mp[34]|webm|wav|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|s[ac]ss|styl)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ]
  },
  "devDependencies": {
    "del-cli": "^3.0.1",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-standard": "^4.0.1",
    "@babel/cli": "^7.12.7",
    "@babel/core": "^7.12.7",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.17",
    "@babel/preset-react": "^7.12.13",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-polyfill": "6.26.0",
    "classnames": "^2.2.6",
    "copy-webpack-plugin": "^5.1.2",
    "css-loader": "^3.6.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-jest": "^23.20.0",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "postcss-cli": "^6.1.3",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "sass-loader": "^8.0.2",
    "semantic-release": ">=17.2.3",
    "serve": "^11.3.2",
    "shx": "^0.3.2",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^4.2.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^3.0.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dashboard": "^3.2.1",
    "webpack-dev-server": "^3.11.0",
    "workbox-webpack-plugin": "^5.1.4"
  }
}
